<div class="col-md-12">
  <div class="panel panel-default">
    <div class="panel-heading">
      <button type="button" class="close" id="closePanel">&times;</button>
      <h3 class="panel-title">Some Interesting Pokemons</h3>
    </div>
    <div class="panel-body">
    {{#this}}
    <div class="col-md-4">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">{{FirstStringUpperCase name}} : {{pokeid}}</h3>
        </div>
        <div class="panel-body">
          <p>Some Description to be added later</p>
        </div>
        <p><a class="btn btn-info pokemontemplate" role="button" data-url="{{url}}" data-name="{{name}}">View details &raquo;</a></p>
      </div>
    </div>
    {{/this}}
    </div>
  </div>
</div>
